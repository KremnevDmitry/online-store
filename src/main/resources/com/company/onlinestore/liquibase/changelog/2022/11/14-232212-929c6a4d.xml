<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
                      http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.11.xsd"
        objectQuotingStrategy="QUOTE_ONLY_RESERVED_WORDS">
    <changeSet id="1" author="online-store">
        <addNotNullConstraint columnDataType="VARCHAR(255)" columnName="FIRST_NAME" tableName="USER_" validate="true"/>
    </changeSet>
    <changeSet id="2" author="online-store">
        <addNotNullConstraint columnDataType="VARCHAR(255)" columnName="LAST_NAME" tableName="USER_" validate="true"/>
    </changeSet>
    <changeSet id="3" author="online-store">
        <addNotNullConstraint columnDataType="VARCHAR(255)" columnName="PASSWORD" tableName="USER_" validate="true"/>
    </changeSet>
</databaseChangeLog>